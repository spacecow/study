<% present glossary do |presenter| %>

<<%= glossary_tag %> class="glossary <%= extra_class %>">
  <%= presenter.content %>
  <%= presenter.parenthesis %>
  <%= presenter.actions %>
  <div class="definition">
    <%= presenter.definition lookup if lookup %>
  </div>
  <%= presenter.sentences if sentences %>
  <%= presenter.kanjis(glossary,:ul) if kanjis %>
</<%= glossary_tag %>>

<% end %>
